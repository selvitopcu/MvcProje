
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<button class="btn btn-primary" id="btn1">Button</button>
<button class="btn btn-info" id="btn2">Button2</button>
<button class="btn btn-danger" id="btn3">Button3</button>

<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje");
    });
</script>
<script>
    $('#btn2').click(function () {
        swal("Merhaba Mvc Proje","Sonuncu Kısım");
    });
</script>
<script>
    $('#btn3').click(function () {
        swal("Merhaba Mvc Proje","Sonuncu Kısım","success");
    });
</script>